    "lint:staged": "lint-staged",
    "lint:fix": "eslint --fix --ext .js,.ts .",
    "prettier:fix": "prettier --write \"src/**/*.{js,ts,css,html,json,md}\"",
    "prettier:check": "prettier \"src/**/*.{js,ts,css,html,json,md}\"",
    "prepare": "husky install"
